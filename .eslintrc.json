{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/no-unused-modules": [1, { "unusedExports": true }],
    "import/no-unresolved": "off",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "plugins": ["import"]
}
